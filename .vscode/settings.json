{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "files.exclude": {
    "**/.git": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/*.json": true,
    "**/build/**": true,
    "**/cdk.out/**": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.formatting.provider": "none"
}
