{
  "name": "cdkapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "8.x",
    "node": "18.x"
  },
  "bin": {
    "server": "bin/cdkApp.js"
  },
  "type": "module",
  "scripts": {
    "fresh-deploy-dev": "cdk bootstrap --profile qr-dev --context deployment-env=development && npm run deploy-dev",
    "synth-dev-no-install": "cdk synth --all --context deployment-env=development --profile qr-dev",
    "synth-dev": "npm run install-all && npm run synth-dev-no-install",
    "deploy-dev-no-install": "cdk deploy --all --profile qr-dev --context deployment-env=development --ci && node ./bin/postDeploy.js development qr-dev",
    "deploy-dev": "npm run install-all && npm run deploy-dev-no-install",
    "deploy-dev-hotswap": "cdk deploy --all --profile qr-dev --context deployment-env=development --hotswap",
    "destroy-dev": "cdk destroy --all --context deployment-env=development --profile qr-dev",
    "fresh-deploy-production": "cdk bootstrap --profile qr-prod --context deployment-env=production",
    "deploy-production-cicd": "cdk deploy --app \"npx node ./bin/deployCiCd.js\" --ci --context deployment-env=production --profile qr-prod",
    "deploy-production-ci": "cdk deploy --all --ci --context deployment-env=production --require-approval never && node ./bin/postDeploy.js production qr-prod",
    "deploy-production": "cdk deploy --all --ci --context deployment-env=production --require-approval never --profile qr-prod && node ./bin/postDeploy.js production qr-prod",
    "destroy-prodution": "cdk destroy --all --ci --context deployment-env=production --profile qr-prod",
    "install-all": "npm install && npm-recursive-install && cd ../castofly-common && npm install",
    "lint": "eslint --ext .js .",
    "test": "echo \"No tests configured\" && exit 0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "ava": "^5.3.1",
    "aws-cdk": "^2.171.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "recursive-install": "^1.4.0",
    "shelljs": "^0.8.5"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.699.0",
    "@aws-sdk/client-dynamodb": "^3.705.0",
    "@aws-sdk/client-lambda": "^3.699.0",
    "@aws-sdk/client-s3": "^3.701.0",
    "@aws-sdk/client-secrets-manager": "^3.699.0",
    "@aws-sdk/client-ses": "^3.699.0",
    "@aws-sdk/client-sfn": "^3.699.0",
    "@aws-sdk/client-sns": "^3.699.0",
    "@aws-sdk/client-ssm": "^3.699.0",
    "@aws-sdk/credential-provider-sso": "^3.395.0",
    "@aws-sdk/lib-dynamodb": "^3.699.0",
    "@aws-sdk/lib-storage": "^3.701.0",
    "@aws-sdk/s3-request-presigner": "^3.701.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "aws-cdk-lib": "^2.171.0",
    "axios": "^1.7.8",
    "bestzip": "^2.2.1",
    "cli-confirm": "^1.0.1",
    "constructs": "^10.2.69",
    "del-cli": "^5.0.0",
    "dotenv": "^16.3.1",
    "git-branch": "^2.0.1",
    "hono": "^4.6.12",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "promise-retry": "^2.0.1",
    "uuid": "^11.0.3"
  }
}
