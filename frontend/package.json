{
  "engines": {
    "npm": "10.x",
    "node": "22.x"
  },
  "name": "frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@openreplay/tracker": "^16.2.1",
    "@reduxjs/toolkit": "^1.8.3",
    "amazon-cognito-identity-js": "^6.3.3",
    "axios": "^0.27.2",
    "browser-image-compression": "^2.0.0",
    "castofly-common": "file:../castofly-common",
    "detect-browser": "^5.3.0",
    "dompurify": "^3.0.3",
    "js-sha256": "^0.9.0",
    "p-limit": "^6.2.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-confetti": "^6.1.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-tooltip": "^5.21.4",
    "reselect": "^4.1.6",
    "sass": "^1.77.2",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "start-app": "npm run copy-dev && cross-env REACT_APP_APPSWITCH=castoflyApp react-scripts start",
    "start-app-npm-package": "npm run copy-dev && cross-env REACT_APP_APPSWITCH=castoflyApp REACT_APP_CASTOFLY_LOCAL_HOST=true react-scripts start",
    "copy-dev": "node ./config/copyConfig.js development",
    "copy-prod": "node ./config/copyConfig.js production",
    "build-app-ci": "CI=false GENERATE_SOURCEMAP=true REACT_APP_APPSWITCH=castoflyApp react-scripts build",
    "build-app-production-ci": "npm run copy-prod && npm run build-app-ci",
    "deploy-app-production-ci": "npm run build-app-production-ci && aws s3 sync ./build \"s3://scriptover.com\" --exclude '*.js.map' --delete && aws cloudfront create-invalidation --no-cli-pager --distribution-id EKTG5QJ32IE8A --paths \"/*\"",
    "upload-app-production-ci": "aws s3 sync ./build \"s3://scriptover.com\" --exclude '*.js.map' --delete && aws cloudfront create-invalidation --no-cli-pager --distribution-id EKTG5QJ32IE8A --paths \"/*\"",
    "profile": "npm run build-app -- --stats && npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "test": "node ./config/copyConfig.js development && jest --passWithNoTests",
    "lint": "eslint --ext .js .",
    "coverage": "node ./config/copyConfig.js development && jest --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.5.0",
    "cli-confirm": "^1.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "node-notifier": "^10.0.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "webpack-bundle-analyzer": "^4.9.1"
  }
}
